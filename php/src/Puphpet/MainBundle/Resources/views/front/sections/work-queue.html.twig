<div class="tab-pane fade in" id="work-queue">
    <div class="page-header compact">
        <h1><i class="fa fa-sort-numeric-asc"></i> Work Queues</h1>
    </div>
    <ul class="nav nav-tabs icon-bar">
        <li class="col-xs-3 active">
            <a href="#beanstalkd" data-toggle="tab">
                <div class="tab-icon">
                    <img src="/assets/v4/img/vendors/beanstalkd-active.png" class="tab-icon-active" />
                    <img src="/assets/v4/img/vendors/beanstalkd-inactive.png" class="tab-icon-inactive" />
                    Beanstalkd
                </div>
            </a>
        </li>
        <li class="col-xs-3">
            <a href="#rabbitmq" data-toggle="tab">
                <div class="tab-icon">
                    <img src="/assets/v4/img/vendors/rabbitmq-active.png" class="tab-icon-active" />
                    <img src="/assets/v4/img/vendors/rabbitmq-inactive.png" class="tab-icon-inactive" />
                    RabbitMQ
                </div>
            </a>
        </li>
    </ul>

    <div class="tab-content">
        <div class="tab-pane active" id="beanstalkd">
            {% set extension = extensionManager.getExtensionBySlug('beanstalkd') %}
            {{ extension.renderFront(extension.data)|raw }}
        </div>
        <div class="tab-pane" id="rabbitmq">
            {% set extension = extensionManager.getExtensionBySlug('rabbitmq') %}
            {{ extension.renderFront(extension.data)|raw }}
        </div>
    </div>

    <ul class="pager">
        <li class="previous"><a href="#database">← Databases</a></li>
        <li class="next"><a href="#search-server">Search Servers →</a></li>
    </ul>
</div>
